// bindMap to Binds

    #map2binds(
        0 |-> a <<- u
        1 |-> b , c , d <<- v
        2 |-> @ \ a /\ String <<- w
        3 |-> e , f , g , h <<- x
        4 |-> p , @ q ! ( \ b /\ Int ) <<- y
        5 |-> @ 1 <<- @ 0
        6 |-> @ 1 <<- @ 0
        7 |-> s , t <<- z
    )

